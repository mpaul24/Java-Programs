package codechef;

import java.math.BigInteger;
import java.util.Scanner;

public class KBIGNUMB {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc = new Scanner(System.in);
		int test = sc.nextInt();
		for(int tt=0;tt<test;tt++)
		{
			int a = sc.nextInt();
			long n  = sc.nextLong();
			int m = sc.nextInt();
			String abc = new String();
			if(n%2==0){
				for(int i=0;i<n/2;i++)
				{
					abc = abc+a+""+a;
				}
			}
			//if(n%2==0) abc=""+a+""+a ;
			else 
			{
				abc = abc+a;
				for(int i=0;i<n/2;i++)
				{
					abc = abc+a+""+a;
				}
			}
			System.out.println(abc);
			BigInteger big1 = new BigInteger(abc);
			BigInteger big3 = new BigInteger(""+m);
			BigInteger big2 = big1.mod(big3);
			System.out.println(big2);
				
		}
	}

}
